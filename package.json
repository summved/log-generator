    "d3fend-list": "ts-node src/cli.ts d3fend-list",
    "d3fend-coverage": "ts-node src/cli.ts d3fend-coverage",
    "performance-test": "ts-node src/cli.ts performance-test",
    "test-generate": "./scripts/timeout.sh 30 sh -c 'npm run generate'",
    "test-replay": "./scripts/timeout.sh 10 sh -c 'npm run replay'",
    "quick-demo": "./scripts/timeout.sh 15 sh -c 'node examples/basic-usage.js'",
    "timed-generate": "./scripts/timed-generate.sh",
    "timed-replay": "./scripts/timed-replay.sh",
    "timed-demo": "./scripts/timed-demo.sh",
    "docker:build": "docker build -t log-generator:latest .",
    "docker:run": "docker run --rm -v $(pwd)/logs:/app/logs log-generator:latest",
