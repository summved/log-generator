    "ml-patterns:config": "ts-node src/cli.ts ml-patterns:config",
    "ml-patterns:reset": "ts-node src/cli.ts ml-patterns:reset",
    "docker:build": "docker build -t log-generator:latest .",
    "docker:run": "docker run --rm -v $(pwd)/logs:/app/logs log-generator:latest"
