{
  "name": "log-generator",
  "version": "1.0.0",
  "description": "Enterprise SIEM log generator with MITRE ATT&CK and D3FEND framework integration - Generate realistic cybersecurity logs for Wazuh, Splunk, ELK Stack testing. Features attack-defense simulation, ML pattern learning, SOC operations, and 12+ log sources for comprehensive security training and purple team exercises",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && mkdir -p dist/config dist/chains/templates && cp src/config/default.yaml dist/config/ && cp src/chains/templates/*.yaml dist/chains/templates/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "generate": "ts-node src/cli.ts generate",
    "replay": "ts-node src/cli.ts replay",
    "validate-config": "ts-node src/cli.ts validate-config",
    "status": "ts-node src/cli.ts status",
    "mitre-list": "ts-node src/cli.ts mitre-list",
