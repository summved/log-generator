    "ml-patterns:status": "ts-node src/cli.ts ml-patterns:status",
    "ml-patterns:generate": "ts-node src/cli.ts ml-patterns:generate",
    "ml-patterns:analyze": "ts-node src/cli.ts ml-patterns:analyze",
    "ml-patterns:config": "ts-node src/cli.ts ml-patterns:config",
    "ml-patterns:reset": "ts-node src/cli.ts ml-patterns:reset",
    "ml-patterns:train-nlp": "ts-node src/cli.ts ml-patterns:train-nlp",
    "ml-patterns:test-anomaly": "ts-node src/cli.ts ml-patterns:test-anomaly",
    "ml-patterns:forecast": "ts-node src/cli.ts ml-patterns:forecast",
